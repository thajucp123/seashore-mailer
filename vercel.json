{
  "version": 2,
  "build": {
    "target": "serverless"
  },
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/send-email",
      "dest": "/app.js"
    },
    {
      "src": "/newsletter",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}
